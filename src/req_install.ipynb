   "source": [
    "in_fn='requirements.txt'\n",
    "\n",
    "\n",
    "with open(in_fn) as f:\n",
    "    lines=f.readlines()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "ll=[l.split( ) for l in lines[2:]]\n",
    "\n",
    "\n",
    "\n",
    "llj=[f'{l[0]}=={l[1]}' for l in ll]\n",
    "\n",
    "llj\n",
    "\n",
    "\n",
    "out_fn='reqs_formatted.txt'\n",
    "\n",
    "\n",
    "with open(out_fn,'w') as f:\n",
    "    for l in llj:\n",
    "        f.write(l+'\\n')"
   ]
